<h1>Novo administrador</h1>

<% form_for(@administrador, :url => admin_administradores_path, :html => { :multipart => true }) do |f| %>
  <%= error_messages_for :administrador %>
  <%= render :partial => "form", :locals => { :f => f } %>
  <%= f.submit "Salvar" %> ou
  <%= link_to "Voltar", admin_administradores_path %>
<% end %>